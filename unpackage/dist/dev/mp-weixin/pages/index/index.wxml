<view class="home"><view class="header"><view value="{{city}}" data-event-opts="{{[['input',[['__set_model',['','city','$event',[]]]]]]}}" bindinput="__e" class="cityName">{{city}}</view><image src="{{widIcon}}" mode="aspectFit"></image><view class="condition"><text>{{info}}</text><text>{{temperature}}</text></view></view><view value="{{nowTemperature}}" data-event-opts="{{[['input',[['__set_model',['','nowTemperature','$event',[]]]]]]}}" bindinput="__e" class="temperature">{{nowTemperature}}</view><button data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">点击发送</button><view class="list"><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li">{{val.week}}</view></block></view><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li">{{val.date}}</view></block></view><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li">{{val.wid.wid1}}</view></block></view><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li"><image src="{{val.widIcon.icon1}}" mode="aspectFit"></image></view></block></view><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li">{{val.temperature}}</view></block></view><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li"><image src="{{val.widIcon.icon2}}" mode="aspectFit"></image></view></block></view><view class="_ul"><block wx:for="{{future}}" wx:for-item="val" wx:for-index="index"><view class="_li">{{val.wid.wid2}}</view></block></view></view><button data-event-opts="{{[['tap',[['goList',['$event']]]]]}}" bindtap="__e">跳转到列表</button></view>